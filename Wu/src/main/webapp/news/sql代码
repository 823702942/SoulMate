create table add_user(id int primary key auto_increment,comp varchar(20),name varchar(10));
create table add_info(id int primary key auto_increment,code varchar(10),name varchar(10),birthday datetime,id_us varchar(20),telephone int,withhold boolean,state varchar(10));
create table add_reno(id int primary key auto_increment,name varchar(10),date_app datetime,bond float(8,2),money_liqu float(8,2),state varchar(10),content varchar(50),name_app varchar(10),time_app datetime);
create table add_admin(id int primary key auto_increment,name varchar(10),date_app datetime,money_app float(8,2),state varchar(10),content varchar(20),name_app varchar(10),date_approval datetime,opinion varchar(20),opinion_accep varchar(20));